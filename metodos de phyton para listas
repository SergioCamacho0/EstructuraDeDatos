numeros = [1,2,3,4,5,6,7]
numeros2 = [9,10,11,12]
print (numeros)
print (numeros[3])
for i in numeros:
    print(i)
print (numeros)
for i in range(len(numeros)):
    print (numeros[i])
numeros.append(8)
print (numeros)
numeros.insert(2,8)
print (numeros)
numeros.remove(8)
print (numeros)
numeros.extend(numeros2)
print (numeros)
lista = numeros * 2
print (lista)
x = lista.count(5)
print (x)
lista.sort()
print (lista)
